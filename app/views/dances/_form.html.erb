<h1>Dances#new</h1>
<p>Find me in app/views/dances/new.html.erb</p>

<div class="">
  <div class="row justify-content-center">
    <div class="col-12 col-lg-6">
      <%= simple_form_for(@dance) do |f| %>
        <div class="row">
          <div class="col-8">
            <%= f.input :fullname %>
          </div>
          <div><%= f.input :fullname, as: :autocomplete, source: autocomplete_dances_path %></div>
          <%= f.submit  class: "btn btn-super"%>
      <% end %>
    </div>
  </div>
</div>
